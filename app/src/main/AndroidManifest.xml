<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myregisterlogin">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="function1.MyStartWalk"></activity>
        <activity android:name="function1.MyStartWalkMain" />
        <activity android:name="main.StartActivity" />
        <activity
            android:name="main.SplashActivity"
            android:screenOrientation="fullSensor"
            android:theme="@style/SplashTheme">
            <intent-filter> <!-- 스플래시 이미지 화면을 첫화면으로 설정하기 -->
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="registerlogin.LoginActivity" />
        <activity android:name="registerlogin.RegisterActivity" />
        <activity android:name="main.MainActivity" />
        <activity android:name="myinformation.MyInformCheck" />
        <activity android:name="myinformation.MyInformChange" />
        <activity android:name="map.MainMap"/>
    </application>

</manifest>